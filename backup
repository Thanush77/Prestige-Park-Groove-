class LabourColonyPage extends StatelessWidget {
  const LabourColonyPage({super.key});

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: const Text('Labour Colony Consumption'),
      ),
      body: SingleChildScrollView(
        child: Column(
          children: [
            SizedBox(
