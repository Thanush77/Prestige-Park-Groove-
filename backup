                  DataRow(cells: [
                    DataCell(Text('2024-11-05')),
                    DataCell(Text('BWSSB')),
                    DataCell(Text('25')),
                    DataCell(Text('Yes')),
                    DataCell(Text('STP')),
                    DataCell(Text('Pipeline')),
                  ]),
                ],
              ),
            ),
          ],
        ),
      ),
    );
  }
}
