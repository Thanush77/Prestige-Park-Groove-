                  ]),
                  DataRow(cells: [
                    DataCell(Text('2024-11-03')),
                    DataCell(Text('Tankers')),
                    DataCell(Text('15')),
                    DataCell(Text('Yes')),
                    DataCell(Text('STP')),
                    DataCell(Text('Tanker')),
                  ]),
                  DataRow(cells: [
                    DataCell(Text('2024-11-04')),
