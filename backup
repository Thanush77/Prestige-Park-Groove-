              child: DataTable(
                columns: const [
                  DataColumn(label: Text('Date')),
                  DataColumn(label: Text('Source')),
                  DataColumn(label: Text('Consumption (KLD)')),
                  DataColumn(label: Text('Discharge')),
                  DataColumn(label: Text('Discharged To')),
                  DataColumn(label: Text('Mode of Transport')),
                ],
